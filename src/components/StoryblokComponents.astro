---
import Task from './Task.astro';
import Column from './Column.astro';
 
const { story } = Astro.props;
---
 
{story?.content?.body?.map((blok: { component: any; }) => {
  switch(blok.component) {
    case 'task':
      return <Task blok={blok} />
      case 'column':
      return <Column blok={blok} />
  }
})}